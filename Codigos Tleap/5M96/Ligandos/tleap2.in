source leaprc.protein.ff19SB
source leaprc.gaff2
source leaprc.water.tip3p

MOL = loadmol2 mol2/Mol2/Mol2.mol2 
loadamberparams mol2/Mol2/Mol2.frcmod
saveamberparm MOL mol2/Mol2/Mol2.prmtop mol2/Mol2/Mol2.inpcrd
saveoff MOL mol2/Mol2/Mol2.lib
loadoff mol2/Mol2/Mol2.lib

protein = loadpdb 5m96.amber.pdb

complex = combine {protein MOL}
savepdb complex mol2/Mol2/5m96_Mol2.pdb
saveamberparm complex mol2/Mol2/5m96_Mol2.prmtop mol2/Mol2/5m96_Mol2.inpcrd

solv_complex = combine {protein MOL}
solvateBox solv_complex TIP3PBOX 8.0
addIons solv_complex Na+ 0
addIons solv_complex Cl- 0
savepdb solv_complex mol2/Mol2/5m96_Mol2_solv.pdb
saveamberparm solv_complex mol2/Mol2/5m96_Mol2_solv.prmtop mol2/Mol2/5m96_Mol2_solv.inpcrd
quit 
